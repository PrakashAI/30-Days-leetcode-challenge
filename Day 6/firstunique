class Solution{
    public:
    
    int firstUniqChar(string  s) {
    
    for(int i=0;i<s.length();i++){
        int flag=1;
      for(int j=0;j<s.length();j++){
          if(i!=j&&s[i]==s[j]){ 
                 flag=0;
                 break;}}
             
            if(flag)
               return i;
    } return -1;
            
            
        
}      
};       
    


